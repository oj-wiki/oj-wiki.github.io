{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "docs/.vitepress/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "docs/.vitepress/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
